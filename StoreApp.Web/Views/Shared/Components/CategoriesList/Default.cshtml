@model IEnumerable<string>

<div class="d-grid gap-2">
    <a class="btn btn-outline-secondary" 
       asp-controller="Home" 
       asp-action="Index">Main Page</a>
    
    @foreach (var category in Model)
    {
        <a class="btn btn-outline-secondary" 
           asp-controller="Home" 
           asp-action="Index">@category</a>
    }
</div>